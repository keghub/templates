{
  "AWS": {
    "Region": "eu-west-1"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "Loggly": {
    "ApiKey": "AppSecret:/emg/static/LogglySecretArn",
    "ApplicationName": "EMG.gRPCService",
    "Environment": "Development"
  }
}
